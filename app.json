{
  "expo": {
    "name": "NHS",
    "slug": "NHS",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xperience.nhs",
      "buildNumber": "2.0.0"
    },
    "android": {
      "package": "com.xperience.nhs",
      "versionCode": 2,
      "permissions":["com.google.android.c2dm.permission.RECEIVE","VIBRATE"],
      "googleServicesFile": "./google-services.json"
    }
  }
}
